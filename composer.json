{
    "name": "hyperf/db",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "",
    "autoload": {
        "psr-4": {
            "Hyperf\\DB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\DB\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "hyperf/config": "~2.1.0",
        "hyperf/contract": "~2.1.0",
        "hyperf/pool": "~2.1.0",
        "hyperf/utils": "~2.1.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/testing": "~2.1.0",
        "mockery/mockery": "^1.0"
    },
    "suggest": {
        "ext-swoole": "Required to use swoole Swoole MySQL."
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "hyperf": {
            "config": "Hyperf\\DB\\ConfigProvider"
        }
    }
}
